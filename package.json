{
	"name": "gulick-group",
	"version": "2.0.9",
	"description": "Gulick Group Wordpress Theme",
	"main": "index.js",
	"scripts": {
			"watch": "node-sass --watch scss/ --output css --output-style=expanded",
			"build": "node-sass scss/ --output css --output-style=expanded",
			"prod": "node-sass scss/ --output css --output-style=compressed",
			"sync": "browser-sync start --proxy gulickgroup.wp.local --files '*.php' 'style.css'",
			"test": "echo \"Error: no test specified\" && exit 1"
	},
	"author": "Brian Cordyack",
	"dependencies": {
			"bootstrap": "latest",
			"popper.js": "^1.14.3"
	}
}
